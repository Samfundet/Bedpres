%h1 Opprett ny bruker

= semantic_form_for @user, :html => { :class => "form-horizontal" } do |f|
  = f.inputs do
    = f.input :email
    = f.input :firstname
    = f.input :surname
    = f.input :password
    = f.input :password_confirmation
  = f.actions do
    = f.action :submit, :lablel => "Ny bruker"
